parameters:
    level: max
    paths:
        - %rootDir%/src/

    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false

    excludePaths:
        # Makes PHPStan crash
        - 'src/DependencyInjection/SyliusBlogExtension.php'

        # Test dependencies
        - 'tests/Application/**/*'
